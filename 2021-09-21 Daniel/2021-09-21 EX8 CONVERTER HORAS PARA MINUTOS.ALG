Algoritmo "ex8 converter horas para minutos"

// Autor(a)    : Lucas Bortoli
// Data atual  : 21/09/2021

Var
   horas, minutos: real
Inicio
   escreva("Duração do evento, em horas: ")
   leia(horas)

   minutos <- horas * 60

   escreval(horas, " hora(s) são", minutos, " minuto(s)")
Fimalgoritmo